<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/after_allelearn"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.a747.smartlearningmanager.After_allelearn">

    <RelativeLayout
        android:layout_width="match_parent"
        android:background="#091534"
        android:padding="5dp"
        android:id="@+id/video_titlebar"
        android:layout_height="50dp"
        android:gravity="center_vertical">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/addTodoBtn"
            android:src="@drawable/backb"
            android:background="#091534"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:onClick="gotoSubjectElearn"
            android:layout_gravity="center"
            android:layout_alignParentBottom="true" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true">

            <ImageButton
                android:layout_width="43dp"
                android:layout_height="match_parent"
                android:id="@+id/imageButton8"
                android:src="@drawable/videob"
                android:layout_gravity="center_horizontal"
                android:background="#091534"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="B.sc??"
                android:id="@+id/subjname"
                android:textColor="#ffffff"
                android:paddingTop="5dp"
                android:textSize="18dp"
                android:layout_alignParentTop="true"
                android:layout_toEndOf="@+id/imageButton30"
                android:layout_marginLeft="10dp"
                android:layout_gravity="center" />
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:background="#ffffff"
        android:layout_height="match_parent"
        android:layout_below="@+id/video_titlebar"
        android:paddingTop="10dp"
        android:paddingBottom="10dp">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/scrollView9"
            android:layout_gravity="center_horizontal"
            android:fillViewport="false">

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="5dp"
                android:id="@+id/tl_datelist">

            </TableLayout>
        </ScrollView>

    </RelativeLayout>

</RelativeLayout>

